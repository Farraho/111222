<script>
    $(function(){
        $(".combination_play").on("submit", function(e){
            e.preventDefault();
            let zone = $(".combination_play .zone input").map((i,el) => {
                if(el.value !== ''){
                return el.value;
              }
            })

            const combination = Array.from(zone).join(',');
            location.href='/6x45/game?combination=' + combination
        })
    })
