<script>
  $(function(){
    function startTimer(duration, display) {
      let timer = duration, hours, minutes, seconds;
      let t = setInterval(function () {
        let seconds = parseInt(timer % 60, 10); // Seconds that cannot be written in minutes
        let secondsInMinutes = (timer - seconds) / 60; // Gives the seconds that COULD be given in minutes
        let minutes = parseInt(secondsInMinutes % 60, 10); // Minutes that cannot be written in hours
        let hours = parseInt((secondsInMinutes - minutes) / 60, 10);

        minutes = minutes < 10 ? "0" + minutes : minutes;
        seconds = seconds < 10 ? "0" + seconds : seconds;

        if (--timer < 0) {
          clearInterval(t);
        } else if(--timer > 86400) {

        } else if(hours > 0) {
          display.textContent = hours + ":" + minutes + ":" + seconds;
        } else {
          display.textContent = minutes + ":" + seconds;
        }

      }, 1000);
    }
    let minutes_5212_1 = 2970,
      display_5212_1 = document.querySelector('.sidebar_timer_rapido2');
    startTimer(minutes_5212_1, display_5212_1);
    let minutes_7103_2 = 234270,
      display_7103_2 = document.querySelector('.sidebar_timer_ruslotto');
    startTimer(minutes_7103_2, display_7103_2);
    let minutes_5536_3 = 0,
      display_5536_3 = document.querySelector('.sidebar_timer_klava');
    startTimer(minutes_5536_3, display_5536_3);
    let minutes_7115_4 = 234270,
      display_7115_4 = document.querySelector('.sidebar_timer_zp');
    startTimer(minutes_7115_4, display_7115_4);
    let minutes_28101_5 = 0,
      display_28101_5 = document.querySelector('.sidebar_timer_1224');
    startTimer(minutes_28101_5, display_28101_5);
    let minutes_28003_6 = 720,
      display_28003_6 = document.querySelector('.sidebar_timer_dvazhdydva');
    startTimer(minutes_28003_6, display_28003_6);
    let minutes_7101_7 = 20670,
      display_7101_7 = document.querySelector('.sidebar_timer_udachanasdachu');
    startTimer(minutes_7101_7, display_7101_7);
    let minutes_1125_8 = 1020,
      display_1125_8 = document.querySelector('.sidebar_timer_keno2');
    startTimer(minutes_1125_8, display_1125_8);
    let minutes_5101_9 = 8070,
      display_5101_9 = document.querySelector('.sidebar_timer_6x45');
    startTimer(minutes_5101_9, display_5101_9);
    let minutes_7105_10 = 234270,
      display_7105_10 = document.querySelector('.sidebar_timer_gzhl');
    startTimer(minutes_7105_10, display_7105_10);
    let minutes_5213_11 = 570,
      display_5213_11 = document.querySelector('.sidebar_timer_rapido-drive');
    startTimer(minutes_5213_11, display_5213_11);
    let minutes_5551_12 = 22470,
      display_5551_12 = document.querySelector('.sidebar_timer_5x2');
    startTimer(minutes_5551_12, display_5551_12);
    let minutes_5536_13 = 0,
      display_5536_13 = document.querySelector('.sidebar_timer_5x36plus');
    startTimer(minutes_5536_13, display_5536_13);
    let minutes_4421_14 = 5070,
      display_4421_14 = document.querySelector('.sidebar_timer_oxota');
    startTimer(minutes_4421_14, display_4421_14);
    let minutes_28075_15 = 120,
      display_28075_15 = document.querySelector('.sidebar_timer_rocketbingo');
    startTimer(minutes_28075_15, display_28075_15);
    let minutes_4420_16 = 2670,
